{
  "name": "google-calendar-time-tracker",
  "version": "1.1.0",
  "description": "Google Calendar Time Tracker summarizes the events for date range.",
  "main": "code.js",
  "scripts": {
    "compile": "node ./scripts/compile.js",
    "prebuild:dev": "npm run compile",
    "prebuild:prod": "npm run compile",
    "build:dev": "cross-env NODE_ENV=dev rollup -c && cross-env NODE_ENV=dev node ./adjustClasp.js",
    "build:prod": "cross-env NODE_ENV=prod rollup -c && cross-env NODE_ENV=prod node ./adjustClasp.js",
    "push:dev": "cd ./dist/dev && npx clasp push",
    "push:prod": "cd ./dist/prod && npx clasp push",
    "deploy:dev": "cd ./dist/dev && npx clasp deploy",
    "deploy:prod": "cd ./dist/prod && npx clasp deploy",
    "reload:dev": "npm run build:dev && npm run push:dev",
    "reload:prod": "npm run build:prod && npm run push:prod"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "Google",
    "Apps",
    "Script",
    "Calendar",
    "time-tracker",
    "Google Calendar"
  ],
  "author": "nathanielc.brewer@gmail.com",
  "license": "See licence in LICENCE",
  "bugs": {
    "url": ""
  },
  "homepage": "https:/www.ncbrewer.ca/time-tracker#readme",
  "devDependencies": {
    "@google/clasp": "^3.0.6-alpha",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-html": "^2.0.0",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-strip": "^3.0.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/google-apps-script": "^2.0.3",
    "cross-env": "^7.0.3",
    "rollup": "^4.50.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@fontsource/roboto": "^5.2.8",
    "html-void-elements": "^3.0.0",
    "jspdf": "^3.0.3",
    "roboto-fontface": "^0.10.0",
    "svg2pdf.js": "^2.6.0"
  }
}
